<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
		<section name="TestDataAccessModel" type="Shaolinq.DataAccessModelConfigurationSectionHandler, Shaolinq"/>
		<section name="TestDataAccessModelPostgresDotConnect" type="Shaolinq.DataAccessModelConfigurationSectionHandler, Shaolinq"/>
		<section name="TestDataAccessModelNamedConnectionString" type="Shaolinq.DataAccessModelConfigurationSectionHandler, Shaolinq"/>
	</configSections>
	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<param name="Header" value="[Header]\r\n"/>
				<param name="Footer" value="[Footer]\r\n"/>
				<param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
			</layout>
		</appender>
		<logger name="Shaolinq.Query">
			<level value="Info"/>
			<appender-ref ref="ConsoleAppender"/>
		</logger>
		<logger name="Shaolinq.ProjectionCache">
			<level value="Error"/>
			<appender-ref ref="ConsoleAppender"/>
		</logger>
	</log4net>
	<connectionStrings>
		<add name="SqliteInMemory" connectionString=":memory:"/>
	</connectionStrings>
	<TestDataAccessModel configSource="TestDataAccessModel.configx"/>
	<TestDataAccessModelPostgresDotConnect SaveAndReuseGeneratedAssemblies="true">
		<SqlDatabaseContexts>
			<Postgres-DotConnect DatabaseName="StaticConfigTest" ServerName="localhost" UserId="postgres" Port="5432" Password="postgres" Pooling="True" MaxPoolSize="64"/>
		</SqlDatabaseContexts>
		<ReferencedTypes>
			<Type Name="Shaolinq.DataAccessModel, Shaolinq"/>
		</ReferencedTypes>
	</TestDataAccessModelPostgresDotConnect>
	<TestDataAccessModelNamedConnectionString>
		<SqlDatabaseContexts>
			<Sqlite ConnectionStringName="SqliteInMemory" />
		</SqlDatabaseContexts>
	</TestDataAccessModelNamedConnectionString>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
	</startup>
	<system.transactions>
		<defaultSettings timeout="00:10:00"/>
	</system.transactions>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Npgsql" publicKeyToken="5d8b90d52f46fda7"/>
				<bindingRedirect oldVersion="3.0.5.0" newVersion="3.1.0.0"/>
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Diagnostics.DiagnosticSource" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>
